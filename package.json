    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "ethers": "^6.13.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "form-data": "^4.0.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
